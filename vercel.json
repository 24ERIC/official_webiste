{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
            
        },
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",   
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"} 
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py" 
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
  
  
  